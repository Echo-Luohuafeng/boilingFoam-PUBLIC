FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      alpha.water;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    walls
    {
        type            constantAlphaContactAngle;
        gradient        uniform 0;
        limit           gradient;
        theta0          30;
        value           uniform 1;
    }

    plexi
    {
        type            constantAlphaContactAngle;
        gradient        uniform 0;
        limit           gradient;
        theta0          30;
        value           uniform 1;
    }

    symm
    {
        type            symmetry;
    }

    inlet
    {
        type        fixedValue;
        value       uniform 1;
    }

    outlet
    {
    type        inletOutlet;
    inletValue 	uniform 1;
    value	uniform 1;
    }
}

