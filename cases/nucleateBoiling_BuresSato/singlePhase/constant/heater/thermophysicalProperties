FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      thermophysicalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

thermoType
{
    type            heSolidThermo;
    mixture         pureMixture;
    transport       constIso;
    thermo          hConst;
    equationOfState rhoConst;
    specie          specie;
    energy          sensibleEnthalpy;
}

mixture
{
    specie
    {
        nMoles      1;
        molWeight   47.87;   // [g/mol]
    }

    transport
    {
        kappa   17;        // [W/m/K]
    }

    thermodynamics
    {
        Hf      0;
        Cp      544;        // [J/kg/K]
    }

    equationOfState
    {
        rho     4510;       // [kg/m^3]
    }
}


